{
  "name": "blog2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "docusaurus": "docusaurus",
    "dev": "docusaurus start --host 0.0.0.0 --port 3000",
    "build": "docusaurus build",
    "build-analyze": "docusaurus build --bundle-analyzer true",
    "swizzle": "docusaurus swizzle",
    "deploy": "cross-env GIT_USER=dhbalaji docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --port 3001",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prepare": "husky",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:debug": "cross-env PWDEBUG=console playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:install": "playwright install"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/plugin-google-gtag": "^3.9.2",
    "@docusaurus/plugin-ideal-image": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@mdx-js/react": "^3.1.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "husky": "^9.1.7",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@playwright/test": "^1.56.1",
    "@tsconfig/docusaurus": "^2.0.7",
    "@types/node": "^24.10.1",
    "babel-plugin-prismjs": "^2.1.0",
    "cross-env": "^7.0.3",
    "docusaurus-plugin-sass": "^0.2.6",
    "mcp-hello-world": "^1.1.2",
    "sass": "^1.94.2",
    "typescript": "^5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "core-js",
      "core-js-pure",
      "@parcel/watcher"
    ]
  }
}